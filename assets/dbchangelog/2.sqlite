insert into person(id, name, info) values(1248, 'Montesquieu', '');
insert into quote(liked, person_id, quote) values(0, 1248, 'En büyük devletler, gençler tarafından yıkıma sürüklenmiş, ihtiyarlar tarafından kurtarılmış ve kalkındırılmıştır.');
insert into quote(liked, person_id, quote) values(0, 631, 'Hükümetler gibi kocalar da, hiç bir zaman yanıldıklarını ifade etmemelidir.');
insert into person(id, name, info) values(1249, 'Fletcher Knebel', '');
insert into quote(liked, person_id, quote) values(0, 1249, 'Devlet adamı olmak, politikacı olmaktan daha güçlüdür, politikacılar halkla iyi geçinmeye bakarlar, devlet adamları ise politikacılarla da iyi geçinmek zorundadırlar.');
insert into quote(liked, person_id, quote) values(0, 1171, 'İdeali olmayan devletler her an kopacak bir kıyameti beklerler.');
