insert into person(id, name, info) values(1250, 'Thomas Browne', '');
insert into quote(liked, person_id, quote) values(0, 1250, 'Geçmişten çok geleceği düşünmeliyiz, çünkü bundan sonra orada yaşayacağız.');
insert into quote(liked, person_id, quote) values(0, 1231, 'Aşağı düzeydeki yaratıkların zekâsını yalnız açlık keskinleştirir. Tok bir hayvan korkunç aptaldır.');
insert into quote(liked, person_id, quote) values(0, 1231, 'Anlamayacaklara anlatma sakın bilebileceğin en güzel şeyleri.');
